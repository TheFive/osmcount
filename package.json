    "codecov.io": "0.0.8",
    "commander": "^2.7.1",
    "debug": "^2.1.1",
    "fs": "0.0.2",
    "mocha-lcov-reporter": "0.0.2",
    "mongodb": "^1.4.30",
    "numeral": "^1.5.3",
